{
  "name": "@textile/threads-client",
  "version": "0.6.14",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build:node",
    "prebuild": "npm run clean",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "npm run compile",
    "build:browser": "webpack --mode production --config ../../webpack.config.js",
    "compile": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": "github:textileio/js-threads",
  "references": [
    {
      "path": "../id"
    },
    {
      "path": "../multiaddr"
    },
    {
      "path": "../core"
    },
    {
      "path": "../context"
    },
    {
      "path": "../security"
    }
  ],
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/context": "^0.4.8",
    "@textile/multiaddr": "^0.0.18",
    "@textile/security": "^0.1.5",
    "@textile/threads-client-grpc": "^0.1.19",
    "@textile/threads-core": "^0.1.25",
    "@textile/threads-id": "^0.1.7",
    "@types/google-protobuf": "^3.7.2",
    "cids": "^0.8.0",
    "google-protobuf": "^3.10.0",
    "next-tick": "^1.1.0",
    "to-json-schema": "^0.2.5"
  },
  "devDependencies": {
    "@types/multibase": "^0.6.0",
    "@types/next-tick": "^1.0.0",
    "@types/to-json-schema": "^0.2.0",
    "isomorphic-ws": "^4.0.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2",
    "ws": "^7.2.3"
  },
  "gitHead": "40d7c6d2c3539aa273c112a6f918983ef704c179"
}
